<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>registerServlet</servlet-name>
        <servlet-class>web1.service.registerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerServlet</servlet-name>
        <url-pattern>/registerServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>web1.service.loginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>welcomeServlet</servlet-name>
        <servlet-class>web1.service.welcomeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>welcomeServlet</servlet-name>
        <url-pattern>/welcomeServlet</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>forgotpasswordservlet</servlet-name>
        <servlet-class>web1.service.forgotpasswordservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>forgotpasswordservlet</servlet-name>
        <url-pattern>/forgotpasswordservlet</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>setpasswordservlet</servlet-name>
        <servlet-class>web1.service.forgotpasswordservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>setpasswordservlet</servlet-name>
        <url-pattern>/setpasswordservlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
